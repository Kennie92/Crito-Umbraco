@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
    //ViewData["title"] = "Welcome";
    Layout = null;
}
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="description" content="@ViewData["PageDescription"]?.ToString() />
        <meta name="keywords" content="@ViewData["PageKeywords"]?.ToString()" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <script
            src="https://kit.fontawesome.com/41c8b474e2.js"
            crossorigin="anonymous"
        ></script>
        <link
            href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css"
            rel="stylesheet"
            integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD"
            crossorigin="anonymous"
        />
        <link rel="stylesheet" href="/css/style.css" />
        <title>@ViewData["PageTitle"]?.ToString() | Crito</title>
    </head>
    <body>
        @await Html.PartialAsync("shared/_NavbarSection")
        
        <main>
            @RenderBody()
        </main>
        
        @await Html.PartialAsync("shared/_FooterSection")
        
        <script
            defer
            src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN"
            crossorigin="anonymous"
        ></script>
    </body>
</html>

